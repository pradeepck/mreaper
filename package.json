{
  "name": "mreaper",
  "version": "0.0.4-10",
  "description": "A web crawler framework",
  "main": "dist/index.js",
  "scripts": {
    "test": "babel-node source/test.js --presets es2015,stage-2",
    "start": "babel-node source/test.js --presets es2015,stage-2",
